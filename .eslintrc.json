{
  "plugins": [
    "@typescript-eslint",
    "simple-import-sort",
    "only-warn",
    "prettier"
  ],
  "rules": {
    "simple-import-sort/imports": [
      "warn",
      {
        "groups": [
          [
            "^\\u0000",
            "^@?\\w",
            "^[^.]",
            "^\\."
          ]
        ]
      }
    ],
    "simple-import-sort/exports": "warn",
    "prettier/prettier": "warn"
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "prettier"
  ]
}